{
  "hosting": {
    "predeploy": "yarn workspace temp-drive-frontend build",
    "public": "frontend/build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "firebase.storage.rules"
  }
}
